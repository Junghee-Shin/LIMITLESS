package com.limitless.reciperesearch;

import com.limitless.cmn.DTO;

public class RecipeResearchVO extends DTO{

	private String reNo;
	private String tool;
	private String dish;
	private String food;
	
	
	
	public RecipeResearchVO(){}



	/**
	 * @param reNo
	 * @param tool
	 * @param dish
	 * @param food
	 */
	public RecipeResearchVO(String reNo, String tool, String dish, String food) {
		super();
		this.reNo = reNo;
		this.tool = tool;
		this.dish = dish;
		this.food = food;
	}



	/**
	 * @return the reNo
	 */
	public String getReNo() {
		return reNo;
	}



	/**
	 * @param reNo the reNo to set
	 */
	public void setReNo(String reNo) {
		this.reNo = reNo;
	}



	/**
	 * @return the tool
	 */
	public String getTool() {
		return tool;
	}



	/**
	 * @param tool the tool to set
	 */
	public void setTool(String tool) {
		this.tool = tool;
	}



	/**
	 * @return the dish
	 */
	public String getDish() {
		return dish;
	}



	/**
	 * @param dish the dish to set
	 */
	public void setDish(String dish) {
		this.dish = dish;
	}



	/**
	 * @return the food
	 */
	public String getFood() {
		return food;
	}



	/**
	 * @param food the food to set
	 */
	public void setFood(String food) {
		this.food = food;
	}



	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "RecipeResearchVO [reNo=" + reNo + ", tool=" + tool + ", dish=" + dish + ", food=" + food + "]";
	}



	/* (non-Javadoc)
	 * @see java.lang.Object#hashCode()
	 */
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((dish == null) ? 0 : dish.hashCode());
		result = prime * result + ((food == null) ? 0 : food.hashCode());
		result = prime * result + ((reNo == null) ? 0 : reNo.hashCode());
		result = prime * result + ((tool == null) ? 0 : tool.hashCode());
		return result;
	}



	/* (non-Javadoc)
	 * @see java.lang.Object#equals(java.lang.Object)
	 */
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		RecipeResearchVO other = (RecipeResearchVO) obj;
		if (dish == null) {
			if (other.dish != null)
				return false;
		} else if (!dish.equals(other.dish))
			return false;
		if (food == null) {
			if (other.food != null)
				return false;
		} else if (!food.equals(other.food))
			return false;
		if (reNo == null) {
			if (other.reNo != null)
				return false;
		} else if (!reNo.equals(other.reNo))
			return false;
		if (tool == null) {
			if (other.tool != null)
				return false;
		} else if (!tool.equals(other.tool))
			return false;
		return true;
	}



	
	
	
	
}
